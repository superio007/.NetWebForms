@model List<WebForm2.Models.Domain.Student>
@{
}
<h1 class="mb-5 align-content-center">All Students Details</h1>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>E-mail</th>
            <th>Phone</th>
            <th>Date Of Birth</th>
            <th> </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var student in Model)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.Name</td>
                <td>@student.Email</td>
                <td>@student.Phone</td>
                <td>@student.DateOfBirth.ToString("dd-mm-yyyy")</td>
                <td><a href="Students/View/@student.Id">view</a></td>
            </tr>
        }
    </tbody>
</table>